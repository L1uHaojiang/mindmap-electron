{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "types/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
    "src/preload/*.d.ts",
    "types/*.d.ts",
    "__tests__/parser_test.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/renderer/src/*"]
    }
  }
}
